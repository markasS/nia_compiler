ЦЕЛОЕ функция (ЦЕЛОЕ порядок, ЦЕЛОЕ сколько) {
  ЕСЛИ (сколько > 0) {
       ПИСАТЬ (порядок);
       функция (порядок + 1, сколько - 1);
  }
  ВЫВОД 1;
}

ЦЕЛОЕ функция2 (ЦЕЛОЕ с, ЦЕЛОЕ строчек ) {
    ЦЕЛОЕ ч = строчек;
    ЕСЛИ (с > 0) {
        функция (с, строчек );
        ПИСАТЬ ();
        с = с-1;
        функция2 (с, строчек);
    }
}

{
функция2 (5, 5);
}
